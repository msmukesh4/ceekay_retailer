<% if !flash[:notice].blank? %>
    <div class="notice">
        <%= flash[:notice] %>
    </div>
<% end %>

<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>


<div class = "well">
   <%= form_tag '/upload/create', { :multipart => false } do %>
    	<p>
    		<%= file_field_tag 'upload[file]' %>
    	</p>
    	<p>
            <%= submit_tag "Upload", :class => "form-button shadow-button" %>
    	</p>

	<% end %>
</div>
